{
  "name": "Music Queue",
  "description": "Base Level Extension",
  "permissions": [
    "contextMenus",
    "storage",
    "identity"
  ],
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": [
    "https://accounts.spotify.com/*",
    "https://api.spotify.com/*"
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "pics/playlist_extension.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}